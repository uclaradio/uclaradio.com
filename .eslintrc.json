{
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5",
        "jsxBracketSameLine": true
      }
    ]
  }
}
